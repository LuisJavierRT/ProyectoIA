<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.luisjavier.application_ia.InfoPanelActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <EditText
        android:id="@+id/usageText"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_above="@+id/eGeneratedText"
        android:layout_alignLeft="@+id/eGeneratedText"
        android:layout_alignStart="@+id/eGeneratedText"
        android:layout_marginBottom="56dp"
        android:layout_marginTop="48dp"
        android:ems="10"
        android:focusable="false"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="@+id/eGeneratedText"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/eGeneratedText"
        app:layout_constraintRight_toRightOf="@+id/greenButton"
        app:layout_constraintTop_toBottomOf="@+id/panelText"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteY="271dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="192dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/panelsolar2"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/panelText"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="21dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/eGeneratedText"
        android:layout_width="152dp"
        android:layout_height="40dp"
        android:layout_above="@+id/greenButton"
        android:layout_alignLeft="@+id/panelText"
        android:layout_alignStart="@+id/panelText"
        android:focusable="false"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="153dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="152dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="319dp"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/redButton"
        android:layout_width="117dp"
        android:layout_height="91dp"
        app:srcCompat="@drawable/redbutton"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/greenButton"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/greenButton"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/greenButton"
        android:layout_width="111dp"
        android:layout_height="85dp"
        android:layout_alignEnd="@+id/redButton"
        android:layout_alignRight="@+id/redButton"
        android:layout_alignTop="@+id/redButton"
        android:layout_marginBottom="28dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/greenbutton"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/text1u"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Usage"
        android:layout_alignBaseline="@+id/usageText"
        android:layout_alignBottom="@+id/usageText"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        android:layout_marginStart="46dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="33dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="46dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Electricity"
        android:layout_alignTop="@+id/eGeneratedText"
        android:layout_alignRight="@+id/textView3"
        android:layout_alignEnd="@+id/textView3"
        app:layout_constraintRight_toLeftOf="@+id/eGeneratedText"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="15dp"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toTopOf="@+id/eGeneratedText"
        android:layout_marginRight="15dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Generated"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/eGeneratedText"
        android:layout_toStartOf="@+id/eGeneratedText"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <EditText
        android:id="@+id/panelText"
        android:layout_width="149dp"
        android:layout_height="39dp"
        android:layout_above="@+id/usageText"
        android:layout_centerHorizontal="true"
        android:focusable="false"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/redButton"
        android:layout_marginTop="187dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/usageText"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteY="189dp"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_editor_absoluteX="143dp" />



</android.support.constraint.ConstraintLayout>
